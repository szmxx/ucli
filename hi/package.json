{
  "name": "hi",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@8.6.11",
  "funding": "https://github.com/sponsors/szmxx",
  "homepage": "https://github.com/szmxx/template-vite#readme",
  "license": "MIT",
  "author": {
    "name": "cola",
    "email": "szmintel@gmail.com",
    "url": "https://aitimi.cn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szmxx/template-vite",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/szmxx/template-vite/issues"
  },
  "keywords": [
    "template",
    "vue3",
    "vite"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,vue,tsx --ignore-path .gitignore .",
    "style": "stylelint src/**/*.{css,scss,vue}",
    "tslint": "vue-tsc --noEmit",
    "commit": "git-cz",
    "test": "vitest dev --run",
    "ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "changelogen": "changelogen --bump",
    "release": "changelogen --release"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.18",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "element-plus": "^2.3.8",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "qiankun": "^2.10.11",
    "universal-cookie": "^6.1.1",
    "vue": "^3.3.4",
    "vue-request": "^2.0.3",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.4.2",
    "@types/nprogress": "^0.2.0",
    "@unocss/reset": "^0.55.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.33.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "changelogen": "^0.5.5",
    "commitizen": "^4.3.0",
    "eslint": "^8.45.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "jsonc-eslint-parser": "^2.3.0",
    "lint-staged": "^13.2.3",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.0",
    "terser": "^5.19.4",
    "typescript": "^5.1.6",
    "unocss": "^0.55.7",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-define-options": "^1.3.11",
    "vite": "^4.4.4",
    "vite-plugin-inspect": "^0.7.33",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.5"
  },
  "lint-staged": {
    "src/**/*.{json,md,vue}": "prettier -w -u",
    "docs/**/*.{json,md,vue}": "prettier -w -u",
    "*.{js,jsx,ts,tsx,vue}": "npm run lint --fix",
    "*.{less,sass,css,scss,vue}": "npm run style --fix"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "inquirer",
        "postcss"
      ]
    }
  }
}